{
  "schema_version": 1,
  "mixin": [
    "mca.mixin.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "mca",
    "version": "${version}",
    "name": "Minecraft Comes Alive",
    "description": "No more squidward-villagers! Interact with male and female human villagers and raise a family.",
    "authors": [
      "Luke100000",
      "S0llace",
      "CDAGaming",
      "ffrannny",
      "jahx_senpoopie",
      "WildBamaBoy",
      "SheWolfDeadly"
    ],
    "contact": {
      "sources": "https://github.com/Luke100000/minecraft-comes-alive",
      "issues": "https://github.com/Luke100000/minecraft-comes-alive/issues"
    },
    "license": "GNU",
    "icon": "mca.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.mca.quilt.MCAQuilt"
      ],
      "client_init": [
        "net.mca.quilt.MCAQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "quilted_fabric_api_base",
        "version": ">=0.1.0"
      },
      {
        "id": "quilted_fabric_registry_sync_v0",
        "version": ">=0.1.0"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.3"
      },
      {
        "id": "architectury",
        "version": ">=7.0.66"
      }
    ]
  }
}